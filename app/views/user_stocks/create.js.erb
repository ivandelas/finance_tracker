document.addEventListener('ajax:success', function() {
  document.getElementById('messages').innerHTML = '<%= j(render(partial: 'layouts/messages.html')) %>';
  <% if @is_profile %>
    document.getElementById('stocks-list').innerHTML = '<%= j(render(partial: 'stocks/profile_list.html')) %>';
  <% else %>
    document.getElementById('stocks-list').innerHTML = '<%= j(render(partial: 'stocks/list.html')) %>';
  <% end %>
  let result = document.getElementById('result')
  if (result) {
    result.innerHTML = '<%= j(render(partial: 'users/result.html')) %>';
  }
});

